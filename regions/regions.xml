       <name>BC Coast Mountains</name>
       <root>https://sites.google.com/site/hikeswithaview/bc-coast-mountains</root>
   </region>

    <region>
        <name>Canadian Rockies</name>
        <root>https://sites.google.com/site/hikeswithaview/canadian-rockies</root>
    </region>

</regions>